		<Nav 
        src="/sunat1.webp"
		title= 'Sunat Khitan Modern Solo'
        text= 'I am a Passionate Developer who is currently based in Sukoharjo, Indonesia.'
        url= 'https://qlm.one/sunatsolo'
        />
		<Hero 
        name= "Sunat Khitan Modern Solo" 
		src = "/sunat1.webp"
		tagline= "Rumah sunat Solo |  Sunat Nyaman, Anak Ceria "
        />
        <Socialicon Medsos={Medsos}/>
<CTA />
